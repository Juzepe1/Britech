<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_template_form_inherit_pp" model="ir.ui.view">
        <field name="name">product.template.form.inherit.pp</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="Properties">
                    <group>
                        <field name="pp_part_number"/>
                        <field name="pp_value"/>
                        <field name="pp_unit"/>
                        <field name="pp_voltage_rating_vdc"/>
                        <field name="pp_dielectric"/>
                        <field name="pp_tolerance"/>
                        <field name="pp_footprint"/>
                        <field name="pp_note"/>
                    </group>
                </page>
            </notebook>
        </field>
    </record>
</odoo>